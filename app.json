{
  "expo": {
    "name": "HA7D",
    "slug": "ha7d",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ha7d",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#29C17E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon":{
      "light": "./assets/logo.png",
      "dark": "./assets/logo.png",
      "tinted": "./assets/logo.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moserjose.ha7d",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Este app precisa de acesso ao microfone para reproduzir Ã¡udio.",
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#29C17E"
      },
      "package": "com.moserjose.ha7d"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#29C17E",
          "dark": {
            "image": "./assets/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#29C17E"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "6a90e9fd-9615-4360-b006-29e563a1a1ad"
      }
    }
  }
}
